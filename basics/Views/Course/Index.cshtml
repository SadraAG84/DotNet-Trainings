@model Course


<h1>course index page test4</h1>


<div class="card mb-3" id="course_@Model.ID">
    <div class="row">
        <div class="col md-4">
            <img src="~/img/@Model.Image" alt="" width="350">
        </div>
        <div class="col md-8">
            <div class="card-body">
                <h5 class="card-title">@Model.Title</h5>
                <p class="card-text">@Model.Desciption</p>
            </div>
        </div>
    </div>
</div>